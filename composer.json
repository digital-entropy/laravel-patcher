{
    "name": "jalameta/jps-patcher",
    "description": "Laravel patch scripts.",
    "require": {
        "illuminate/support": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/console": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "laravel/framework": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rifki Alhuraibi",
            "email": "veelasky@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jalameta\\Patcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jalameta\\Patcher\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Jalameta\\Patcher\\PatcherServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-1.x": "1.1-dev",
            "dev-master": "2.1-dev"
        }
    }
}
